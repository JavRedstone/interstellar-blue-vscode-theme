{
    "name": "interstellar-blue",
    "displayName": "Interstellar Blue",
    "description": "A theme with colors based on the movie Interstellar, but blue.",
    "version": "0.1.0",
    "publisher": "JavRedstone",
    "license": "SEE LICENSE IN LICENSE.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/JavRedstone/interstellar-blue-vscode-theme"
    },
    "keywords": [
        "Theme",
        "Dark Theme",
        "Cool",
        "Contrast",
        "Blue",
        "Purple",
        "Interstellar"
    ],
    "scripts": {
        "vscode:prepublish": "npm run esbuild-base -- --minify",
        "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
        "esbuild": "npm run esbuild-base -- --sourcemap",
        "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
        "test-compile": "tsc -p ./"
    },
    "galleryBanner": {
        "color": "#5c00ff",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Themes"
    ],
    "icon": "icon.png",
    "contributes": {
        "themes": [
            {
                "label": "Interstellar Blue",
                "uiTheme": "vs-dark",
                "path": "./themes/interstellar-blue-color-theme.json"
            }
        ]
    }
}
